<%------------------------------------------------------------------------------------------------------------------------
---- TUI GRID SET START
--------------------------------------------------------------------------------------------------------------------------%>
<link type="text/css" rel="stylesheet" href="/tui/tui-grid/dist/tui-grid.css" />
<link type="text/css" rel="stylesheet" href="/tui/tui-date-picker/dist/tui-date-picker.css" />
<link type="text/css" rel="stylesheet" href="/tui/tui-time-picker/dist/tui-time-picker.css" />
<style>
	.objective.tui-grid-cell {
		/*background: #efefef;*/
		background: #dbdbdb;
	}
	.ownedObjective.tui-grid-cell {
		/*background: #dcecfd;*/
		background: #dbdbdb;
	}
	.keyResult.tui-grid-cell {
		/*background: #efefef;*/
		background: #efefef;
	}
	.ownedKeyResult.tui-grid-cell {
		/*background: #dbdbdb;*/
		background: #efefef;
	}
	.selected.tui-grid-cell:not(.tui-grid-cell-row-header) {
		background: #111f94 !important;
		color: #fdfdfd !important;
	}
	.selectedCell.tui-grid-cell:not(.tui-grid-cell-row-header) {
		color: #fdfdfd !important;
	}
	.tui-grid-cell {
		border:unset;
	}
	.tui-grid-cell-editable {
		border:1px solid #efefef;
		background: #f6f7f8 !important;
	}
	.tui-grid-border-line-top{
		border-top-color: #fff !important;
	}
	.tui-grid-layer-focus-deactive,
	.tui-grid-layer-focus-border{
		/*background-color: #e7f2fd !important; !*for selected*!*/
		display: none !important;
	}

	.tui-grid-rside-area {
		/*border: 1px solid #000;*/
		/*border-radius: 8px;*/
	}
</style>

<script src="/tui/tui-pagination/dist/tui-pagination.js"></script>
<script src="/tui/@toast-ui/calendar/dist/moment.min.js"></script>
<script src="/tui/@toast-ui/calendar/dist/chance.min.js"></script>
<script src="/tui/tui-time-picker/dist/tui-time-picker.js"></script>
<script src="/tui/tui-date-picker/dist/tui-date-picker.js"></script>
<script src="/tui/tui-grid/dist/tui-grid.js"></script>
<script>
	tui.Grid.setLanguage('ko');
	var TimePicker = tui.TimePicker;
	var DatePicker = tui.DatePicker;

	tui.Grid.applyTheme('default',{
		selection: {
			background: '#fff',
			border: '#fff',
		},
		scrollbar: {
			background: '#fff',
			thumb: '#eee',
			active: '#eee'
		},
		row: {
			hover: {
				background: 'none',
			},
		},
		area: {
			header: {
				border: '#fff',
			},
			summary: {
				border: '#fff',
			},
		},
		cell: {
			normal: {
				background: '#f8f9fa',
				showVerticalBorder: true,
				/*showHorizontalBorder: false,*/
			},
			header: {
				background: '#fff',
				border: '#fff',
				/*showVerticalBorder: true*/
			},
			rowHeader: {
				background: '#fff',
				border: '#fff',
				showVerticalBorder: true
			},
			summary: {
				background: '#dedcd3',
				border: '#cfcdc7',
				showVerticalBorder: true,
			},
			selectedHeader: {
				background: '#fff',
			},
			selectedRowHeader: {
				background: '#fff',
			},
			editable: {
				background: '#fff'
			},
			selectedHeader: {
				background: '#fff'
			},
			focused: {
				border: '#fff',
			},
			disabled: {
				background: '#f6f7f8',
				text: '#333333'
			},
			required: {
				background: '#fff',
			},
			invalid: {
				background: '#ffe5e5',
			},
		},
	});
</script>
