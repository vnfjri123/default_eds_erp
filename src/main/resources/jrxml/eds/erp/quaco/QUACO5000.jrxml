	<queryString>
		<![CDATA[SELECT /* selectQUACO5000 */
        corpCd
        ,busiCd
        ,insp_no
		,DATE_FORMAT(STR_TO_DATE(insp_dt, '%Y%m%d'), '%Y-%m-%d') AS insp_dt
        ,(SELECT commCdNm
		FROM TB_SYSTEM_SHA_LIST T3 INNER JOIN TB_SYSTEM_SHA_DET T4 ON T3.groupNo = T4.groupNo
		WHERE T3.groupCd='COM015'
			AND T4.commCd = T1.insp_divi
	  	) AS insp_divi
        ,pic_cd
        ,(SELECT T0.empNm FROM TB_BASE_USER_LIST T0 WHERE T0.empCd = T1.pic_cd) as picNm
        ,depaCd
        ,(SELECT T0.depaNm FROM TB_BASE_DEPA_LIST T0 WHERE T0.corpCd=$P{corpCd} AND T0.depaCd=T1.depaCd) as depaNm
        ,empCd
        ,(SELECT T0.empNm FROM TB_BASE_USER_LIST T0 WHERE T0.empCd = T1.empCd) as empNm
        ,remark
        FROM
        t_mesquaco08 T1
        WHERE 1=1
        AND corpCd = $P{corpCd}
        AND busiCd = $P{busiCd}
        AND T1.insp_no = $P{inspNo}
        ORDER BY insp_no DESC]]>
	</queryString>
