	<queryString>
		<![CDATA[SELECT 
            T1.corpCd
 	    ,CAST(AES_DECRYPT(UNHEX(T3.corpNo),  'edsinc.co.kr!@#') AS CHAR) AS corpNo
	    ,T3.busiNm AS corpNm
	    ,T3.ownerNm
            ,CAST(AES_DECRYPT(UNHEX(T3.zipNo),  'edsinc.co.kr!@#') AS CHAR) AS zipNo
	    ,CAST(AES_DECRYPT(UNHEX(T3.addr),  'edsinc.co.kr!@#') AS CHAR) AS addr
	    ,CAST(AES_DECRYPT(UNHEX(T3.addrDetail),  'edsinc.co.kr!@#') AS CHAR) AS addrDetail
	    ,CAST(AES_DECRYPT(UNHEX(T3.telNo),  'edsinc.co.kr!@#') AS CHAR) AS telNo
	    ,CAST(AES_DECRYPT(UNHEX(T3.faxNo),  'edsinc.co.kr!@#') AS CHAR) AS faxNo
            ,CAST(AES_DECRYPT(UNHEX(T4.corpNo),  'edsinc.co.kr!@#') AS CHAR) AS corpNo2
	    ,T3.busiType
            ,T3.busiItem
	    ,CAST(AES_DECRYPT(UNHEX(T4.corpNo),  'edsinc.co.kr!@#') AS CHAR) AS corpNo2
	    ,T4.custNm
            ,T4.ownerNm AS ownerNm2
            ,CAST(AES_DECRYPT(UNHEX(T4.zipNo),  'edsinc.co.kr!@#') AS CHAR) AS zipNo2
	    ,CAST(AES_DECRYPT(UNHEX(T4.addr),  'edsinc.co.kr!@#') AS CHAR) AS addr2
	    ,CAST(AES_DECRYPT(UNHEX(T4.addrDetail),  'edsinc.co.kr!@#') AS CHAR) AS addrDetail2
	    ,CAST(AES_DECRYPT(UNHEX(T4.telNo),  'edsinc.co.kr!@#') AS CHAR) AS telNo2
	    ,CAST(AES_DECRYPT(UNHEX(T4.faxNo),  'edsinc.co.kr!@#') AS CHAR) AS faxNo2
            ,T4.busiType AS busiType2
            ,T4.busiItem AS busiItem2
	    ,T1.busiCd
            ,T1.sale_close_no
            ,T1.sale_close_dt
            ,T1.tax_divi
            ,T1.sale_close_divi
            ,T1.custCd
	    ,DATE_FORMAT(T1.sale_close_dt, '%m') AS month2
	    ,DATE_FORMAT(T1.sale_close_dt, '%d') AS day2
	    ,(select cast(sum(T0.supAmt + T0.tax)AS int) from t_messalma12 T0 WHERE T0.corpCd = T1.corpCd AND T1.sale_close_no = T0.sale_close_no) AS totalAmount
	    ,(select cast(sum(T0.tax)AS int) from t_messalma12 T0 WHERE T0.corpCd = T1.corpCd AND T1.sale_close_no = T0.sale_close_no) AS totalTax
            ,T1.pic_cd
            ,(SELECT T0.empNm FROM TB_BASE_USER_LIST T0 WHERE T0.empCd = T1.pic_cd) as picNm
            ,T1.depaCd
            ,(SELECT T0.depaNm FROM TB_BASE_DEPA_LIST T0 WHERE T0.corpCd=$P{corpCd} AND T0.depaCd=T1.depaCd) as depaNm
            ,T1.empCd
            ,(SELECT T0.empNm FROM TB_BASE_USER_LIST T0 WHERE T0.empCd = T1.empCd) as empNm
            ,T1.remark
            ,T1.close_yn
	    ,T2.itemCd
            ,(SELECT T0.itemNm FROM TB_BASE_ITEM_LIST T0 WHERE T0.corpCd=$P{corpCd} AND T0.itemCd = T2.itemCd) as itemNm
	    ,(SELECT T0.stan FROM TB_BASE_ITEM_LIST T0 WHERE T0.corpCd=$P{corpCd} AND T0.itemCd = T2.itemCd) as stan
	    ,T2.qty
            ,T2.upr
            ,T2.supAmt
            ,T2.tax
            ,T2.totAmt
        FROM
            t_messalma11 T1 INNER JOIN t_messalma12 T2 ON T1.corpCd = T2.corpCd AND T1.sale_close_no = T2.sale_close_no
            INNER JOIN TB_BASE_BUSI_LIST T3 ON T1.corpCd = T3.corpCd AND T1.busiCd = T3.busiCd
	    INNER JOIN TB_BASE_CUST_LIST T4 ON T1.custCd = T4.custCd
	WHERE 1=1
        AND T1.corpCd = $P{corpCd}
        AND T1.busiCd = $P{busiCd}
	AND T1.sale_close_no = $P{saleCloseNo}
	ORDER BY T1.sale_close_no DESC]]>
	</queryString>
